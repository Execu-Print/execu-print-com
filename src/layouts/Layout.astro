	<body>
		<!-- <Umami id="dfddd9c4-bce1-412d-b554-1484e0222d0b" src="https://cloud.umami.is/script.js" /> -->
		<div class="filter"></div>
		<Banner></Banner>
		<div class="outlet">
			<slot />
		</div>
		<Footer></Footer>
	</body>
</html>
<style is:global>
	html, body {
		margin: 0;
	}
	* {
		/* font-family: Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif; */
		font-family: "Roboto Condensed", Impact, sans-serif;
	}

	.color-red {
		color: red;
	}

	.gallery {
		display: flex;
		flex-wrap: wrap;
		justify-content: center;
	}

	img {
		max-width:100%;
		height: auto;
	}

body {
	background: radial-gradient(circle at bottom ,#eee, #eee 10%,transparent 10%, transparent 20%, #eee 20%, #eee 30%, transparent 30%, transparent 40%, #eee 40%, #eee 50%, transparent 50%, transparent 60%, #eee 60%, #eee 70%, transparent 70%, transparent 80%, #eee 80%, #eee 90%, transparent 90%);
        background-size: 3em 3em;
        background-color: #ffffff;
        opacity: 1;
		/* backdrop-filter: blur(3.5px); */
}



</style>
<style>
	.filter {
		position:fixed;
		width: 100%;
		height: 100%;
		backdrop-filter: blur(3.5px);
		z-index: -1;
	}
	.outlet {
		min-height: 85vh;
		padding: 20px 10px;

		/* backdrop-filter: blur(3.5px); */
		/* background: radial-gradient(circle at bottom ,#eee, #eee 10%,transparent 10%, transparent 20%, #eee 20%, #eee 30%, transparent 30%, transparent 40%, #eee 40%, #eee 50%, transparent 50%, transparent 60%, #eee 60%, #eee 70%, transparent 70%, transparent 80%, #eee 80%, #eee 90%, transparent 90%);
        background-size: 3em 3em;
        background-color: #ffffff;
        opacity: 1; */
	}
</style>
