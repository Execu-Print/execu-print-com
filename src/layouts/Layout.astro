			}
			mixpanel.init('c42a5c4758eae329c3b325b7bb2968fa', {
				debug: true,
				track_pageview: true,
				persistence: 'localStorage',
				api_host: `https://execu-print-contact-worker.abane94.workers.dev/api/mix-event`,
				api_routes: {
